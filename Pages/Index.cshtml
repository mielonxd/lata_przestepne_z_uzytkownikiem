@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
<h1>Dzisiejsze wpisy:</h1>
@foreach (var person in Model.Records.People)
{
  <p>
    @Html.DisplayFor(modelItem => person.FullName)
    @Html.DisplayFor(modelItem => person.Year)
    @Html.DisplayFor(modelItem => person.localDate)
  </p>
}
</div>